# Anthropic API Key (required)
# Used for Claude summaries and Browser Use
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Browser Use API Key (required)
# Same as ANTHROPIC_API_KEY - Browser Use uses Anthropic's Claude
BROWSER_USE_API_KEY=your-anthropic-api-key-here

# Enable/Disable Browser Use (optional, default: true)
# Set to false to use traditional BeautifulSoup scraping
USE_BROWSER_USE=true

# Galileo API Key (optional)
# Used for reliability checks/evaluation
# GALILEO_API_KEY=your-galileo-key-here
# GALILEO_ENDPOINT=https://api.galileo.ai/v1/evaluate
